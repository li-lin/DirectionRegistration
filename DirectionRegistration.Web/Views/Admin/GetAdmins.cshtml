@using DirectionRegistration.Repository.Entities
@model IList<Admin>
@{
    string cu = Session["admin"] as string;               
}
@foreach (Admin d in Model)
{
    if (d.LoginName != "jc_super") { 
        <tr>
            <td class="col-md-8">@d.LoginName</td>
            <td class="col-md-4">
                @if(cu!=d.LoginName){
                    @Ajax.ActionLink("删除", "AdminDel",
                new { Id = d.Id },
        new AjaxOptions() { UpdateTargetId = "admins", LoadingElementId = "loading", Confirm = "确定要删除吗？" },
                new { @class = "btn btn-success" })
                }
            </td>
        </tr>
    }
}